{
  "name": "submariner",
  "version": "0.0.1",
  "description": "Subtitle converting server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server | bunyan",
    "test": "DISABLE_LOG=1 mocha test/index.js --exit --timeout 5000 $MOCHA_OPTIONS",
    "code-format": "./node_modules/.bin/prettier --use-tabs --tab-width 4 --single-quote --trailing-comma all --write '{lib,test}/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jalieven/submariner"
  },
  "bugs": {
    "url": "https://github.com/jalieven/submariner/issues"
  },
  "keywords": [
    "subtitles",
    "convert",
    "conversion",
    "subrip",
    "srt",
    "webvtt",
    "vtt",
    "server"
  ],
  "author": "Jan Lievens",
  "license": "WTFPL",
  "dependencies": {
    "boom": "7.1.1",
    "bunyan": "1.8.12",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-compress": "2.0.0",
    "koa-mount": "^3.0.0",
    "koa-response-time": "2.0.0",
    "koa-router": "7.3.0",
    "koa-static": "^4.0.2",
    "koa-views": "6.1.3",
    "lodash": "4.17.4",
    "prettier": "^1.9.2",
    "pumpify": "^1.3.5",
    "request": "^2.83.0",
    "split2": "^2.2.0",
    "through2": "^2.0.3",
    "to-utf-8": "^1.3.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "crlf": "^1.1.1",
    "mocha": "4.1.0",
    "nodemon": "1.14.9",
    "supertest": "3.0.0"
  }
}
